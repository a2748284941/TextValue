{
  "name": "textvalue-ai-writer",
  "version": "1.0.5",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:prod": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "analyze": "npx vite-bundle-analyzer",
    "deploy:github": "npm run build:prod && gh-pages -d dist",
    "deploy:netlify": "npm run build:prod && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build:prod && vercel --prod"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "element-plus": "^2.4.0",
    "highlight.js": "^11.9.0",
    "marked": "^9.1.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-draggable-plus": "^0.3.0",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/crypto-js": "^4.2.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.3.0",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "gh-pages": "^6.0.0",
    "jsdom": "^22.1.0",
    "netlify-cli": "^17.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.1",
    "typescript": "~5.2.0",
    "vercel": "^32.0.0",
    "vite": "^4.4.11",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}